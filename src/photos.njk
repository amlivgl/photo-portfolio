{% extends "base.njk" %}

{% block body %}
    <header>
         <div class="headline">
            <h1>PHOTOGRAPHY</h1>
            <h2><a class="backlink" href="/">BACK TO EXHIBITIONS</a></h2>
        </div>
    </header>

    {% for image in imagePhotos %}
        <img class="photos" src="images/photo/{{image.src}}" alt="{{image.alt}}" title="{{image.title}}"> 
        <span class="caption">{{image.title}}</span>
    {% endfor %}
{% endblock %}